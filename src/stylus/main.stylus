@charset "UTF-8"

@font-face
	font-family "Yu Gothic"
	src local("Yu Gothic Medium")
	font-weight 400

html
	height 100%

body
	padding 0
	margin 0
	height 100%
	font 14px "Yu Gothic"
	cursor default

table
	margin-top 10px

caption
	text-align left

#search
	padding 8px

#target
	width 100%
	.checkbox
		width 20px
		height 21px
	.label:hover
		background #000
		color #fff
	label
		display inline-block
		width 100%
